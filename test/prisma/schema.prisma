datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  published Boolean @default(false)
  User      User?   @relation(fields: [userId], references: [id])
  userId    Int?
}

model Post2 {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  published Boolean @default(false)
  User      User?   @relation(fields: [userId], references: [id])
  userId    Int?
}

model User {
  id     Int     @id @default(autoincrement())
  name   String
  email  String  @unique
  posts  Post[]
  posts2 Post2[]
}
