model User {
  id     Int     @id @default(autoincrement())
  name   String
  email  String  @unique
  posts  Post[]
  posts2 Post2[]
}

//Alias
model Post {
  id     Int   @id @default(autoincrement())
  User   User? @relation(fields: [userId], references: [id])
  userId Int?
}

//Alias
model Post2 {
  id     Int   @id @default(autoincrement())
  User   User? @relation(fields: [userId], references: [id])
  userId Int?
}
